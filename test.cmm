
/*   this program
     illustrates evaluation of 
     arithmetic and boolean
     expressions
*/
void main()
{
  int m, n, p;
  bool a, b, c;
  scan(m);
  print(m);
  scan(n);
  print(n);
  p = -m - 3 + n / 2 * (-5 + m * n % 4);
  print(p);
  a = m > n || n >= p;
  if (a)
    print(1);
  else
    print(0);
  b = m < n && n <= p;
  if (b == true)
    print(1);
  else
    print(0);
  c = a && !b || !a && b;
  if (c != false)
    print(1);
  else
    print(0);
}